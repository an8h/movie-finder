{
    "parser": "babel-eslint",
    "extends": ["airbnb", "prettier"],
    "plugins": ["prettier"],
    "rules": {
      "prettier/prettier": ["error"],
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
    }, 
    "parserOptions": { "sourceType": "module" },  
    "env": {
        "browser": true,
        "node": true,
        "jest": true
    }
  }